{
  "name": "draco-vis",
  "version": "0.0.12",
  "description": "Draco on the web.",
  "main": "build/draco.js",
  "unpkg": "build/draco.js",
  "jsdelivr": "build/draco.js",
  "module": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "concat": "bash concat_lp.sh ../asp src",
    "build": "rm -rf build && tsc && rollup -c",
    "format": "tslint -p . --fix && prettier --write '{src,test}/**/*.ts'",
    "lint": "tslint -p . && prettier --list-different '{src,test}/**/*.ts'",
    "watch": "npm run build && concurrently --kill-others -n Typescript,Rollup 'tsc -w' 'rollup -c -w'",
    "start": "parcel demo/index.html"
  },
  "author": "Dominik Moritz",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.1",
    "prettier": "^1.14.3",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "<rootDir>/build",
      "src"
    ]
  },
  "dependencies": {
    "datalib": "^1.9.1",
    "draco-vis": "^0.0.9",
    "vega-lite": "^3.0.0-rc6",
    "vega-typings": "^0.3.47",
    "wasm-clingo": "0.2.2"
  }
}
